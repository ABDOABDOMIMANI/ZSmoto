package com.motoinventory.model;

@Entity
@Table(name = "roles")
public class Role {
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Integer id;

    @Enumerated(EnumType.STRING)
    @Column(length = 20)
    private ERole name;

    // Constructors, getters, setters
}

public enum ERole {
    ROLE_USER,
    ROLE_ADMIN
}